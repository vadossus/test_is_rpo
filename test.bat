:main
cls
echo.
echo === VOPROS CTRL+C I PRESS ENTER ===
echo (esli viiti nado pishi exit)
echo.

powershell -noprofile -command "try { $text = Get-Clipboard -Format Text -ErrorAction Stop; if ($text) { $text } else { '' } } catch { '' }" > "%temp%\clip.txt"
set /p "question=" < "%temp%\clip.txt"
del "%temp%\clip.txt"

if "!question!"=="" (
    echo Text CTRL+C!
    pause
    goto main
)

if /i "!question!"=="exit" exit /b


set "question=!question:"=!"
set "question=!question:  = !"
set "question=!question: = !"

set "found="
for /l %%i in (1,1,100) do (
    if defined Q%%i (
        set "db_q=!Q%%i!"
        set "db_q=!db_q:"=!"
        set "db_q=!db_q:  = !"
        
        if "!db_q!"=="!question!" (
            set "answer=!A%%i!"
            set "found=1"
            goto display
        )
    )
)

if not defined found (
    for /l %%i in (1,1,100) do (
        if defined Q%%i (
            set "db_q=!Q%%i!"
            set "db_q=!db_q:"=!"
            set "db_q=!db_q:  = !"
            
            if /i "!db_q!"=="!question!" (
                set "answer=!A%%i!"
                set "found=1"
                goto display
            )
        )
    )
)

:display
cls
echo.
if defined found (
    echo Vopros: "!question!"
    echo.
    echo Otvet: "!answer!"
) else (
    echo Vopros ne naiden: "!question!"
    echo.
    echo Prover vopros, mb ti ego coryavo skopiroval
)
echo.
pause
goto main

